{
  "expo": {
    "name": "cargo",
    "slug": "cargo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/카GO바지.svg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "이 앱은 현재 위치를 사용하여 휴게소와의 거리 정보를 제공합니다."
      } 
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/카GO바지logo2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shinsuji.cargo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/카GO바지logo2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/카GO바지.svg",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#fff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6036fadc-398c-4e35-ba3d-8052421bac5c"
      }
    }
  }
}
